        var errmsg;
        //Add Project Button Event start


        var totalExpense = 0;


        $("#btnaddtogrid").click(function (e) {
            if (!(gridValidation($("#itemIdx option:selected").val()))) {
                // alert("Product Alreaty Exist")
                displayerror();
