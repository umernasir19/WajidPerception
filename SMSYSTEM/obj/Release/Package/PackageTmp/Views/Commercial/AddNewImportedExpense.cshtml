                $('#example2 tbody').empty();
                GetPODataSet(ddvaue)
                // $('#btnaddtogrid').attr('disabled', true);
            }
            else {

                $('#totalTD').val('0.00');
                $('#grandTotalAVPKR').val('0.00');
                clearBottomRows();
                $('#example2 tbody').empty();
            }

        });

        function GetPODataSet(poid) {
