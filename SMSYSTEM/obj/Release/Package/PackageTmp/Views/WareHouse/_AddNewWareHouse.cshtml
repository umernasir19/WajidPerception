        <div class="col-sm-6">
            <!-- text input -->
            <div class="form-group">
                <label>Branch*:</label>
                @Html.DropDownListFor(model => model.BranchIdx, new SelectList(Model.BranchLists, "idx", "branchName"), "Select Branch", new { @Class = "form-control font-weight-bold" })
                @*<input type="text"  class="form-control" placeholder="@ViewBag.CustomerCode" disabled>*@
            </div>
            @Html.ValidationMessageFor(model => model.BranchIdx, "", new { @class = "text-danger" })
